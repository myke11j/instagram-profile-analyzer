{
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 2,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 2,
      "no-console": 1,
      "no-plusplus": 0,
      "no-unused-expressions": 2,
      "indent": 0,
      "semi": 0,
      "linebreak-style": 0,
      "max-len": 1,
      "quote-props": 0
    }
  }